  "homepage": "https://github.com/ShababulAlam/vue2-expand-on-tap#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "keywords": [
    "collapsable",
    "vue2",
    "expand",
    "tap",
    "expandable",
    "expand on tap",
    "tap to expand",
    "read more"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
